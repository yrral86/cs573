<h1><%= params[:method] %></h1>
<div class="sample_sequences">
<h3>Good Sequences</h3>
<% @good_sequences.each do |s| %>
<%= link_to s.to_csv, s %>
<div class="stats"><%= s.percent_human %>%
<%=  "(#{s.human_trial_count}/#{s.trial_count})" %></div>
<div class="clear"></div>
<% end %>
<h3>Bad Sequences</h3>
<% @bad_sequences.each do |s| %>
<%= link_to s.to_csv, s %>
<div class="stats"><%= s.percent_human %>%
<%=  "(#{s.human_trial_count}/#{s.trial_count})" %></div>
<div class="clear"></div>
<% end %>
