<h1>Listing sequences</h1>

<table>
  <tr>
    <th>Src</th>
    <th>Chord 1</th>
    <th>Chord 2</th>
    <th>Chord 3</th>
    <th>Chord 4</th>
    <th>Chord 5</th>
    <th>Chord 6</th>
    <th>Chord 7</th>
    <th>Chord 8</th>
    <th>Chord 9</th>
    <th>Chord 10</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @sequences.each do |sequence| %>
  <tr>
    <td><%= sequence.src %></td>
    <% sequence.ordered_chords.each do |chord| %>
      <td><%= chord.name %></td>
    <% end %>
    <td><%= link_to 'Show', sequence %></td>
    <td><%= link_to 'Edit', edit_sequence_path(sequence) %></td>
    <td><%= link_to 'Destroy', sequence, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Sequence', new_sequence_path %>
